import java.util.*;
import java.util.Scanner;
class Program1
{
	public static void main(String[] args) 
	{
		Scanner scan=new Scanner(System.in);
		double a=scan.nextDouble();
		double b=scan.nextDouble();
		char c=scan.next().charAt(0);

		double ans=0;
		switch(c)
		{
			case '+':
				ans=a+b;
			break;
			
			case '-':
				ans=a-b;
			break;

			case '*':
				ans=a*b;
			break;

			case '/':
				ans=a/b;
			break;
		}
		System.out.print(ans);
		


	}
}
